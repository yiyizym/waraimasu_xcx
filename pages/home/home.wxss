@import "../common/lib/weui.wxss";

swiper {
  box-sizing:border-box;  
  height: 100vh;
  padding:2vh;
}
.swiper-item{
  position: relative;
  box-sizing:border-box;  
  display: block;
  min-height: 100%;
  padding: 3vh 1vh 3vh 3vh;
  background:#fff;
}
.content {
  font-size: 22px;
  line-height: 1.8;
}
.btn_ctn {
  position: absolute;
  display: flex;
  justify-content: space-around;
  bottom: 10vh;
  width: 100%;
  left: 50%;
  transform: translate3d(-50%, 0, 0);
  text-align: center;
}

.share_btn {
  box-sizing: border-box;
  width: 50px;
  height: 50px;
  border: 1px solid #1eef4f;
  background-color: #fff;
  background-repeat: no-repeat;
  background-position: 48% 40%;
  background-size: 50% 50%;
  background-image: url('data:image/png;base64,');
}

.favorite_btn {
  width: 50px;
  height: 50px;
  padding: 0;
  border: 1px solid #999;
  background-color: #fff;
  background-repeat: no-repeat;
  background-position: 48% 40%;
  background-size: 50% 50%;
  background-image: url('data:image/png;base64,');
}

.favorite_btn.active {
  border: 1px solid #1eef4f;
  background-image: url('data:image/png;base64,');
}